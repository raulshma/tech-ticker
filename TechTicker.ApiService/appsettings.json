{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "TechTicker-Super-Secret-Key-For-JWT-Token-Generation-2024",
    "Issuer": "TechTicker",
    "Audience": "TechTicker-Users",
    "DurationInMinutes": 1440
  },
  "ProductDiscovery": {
    "MaxConcurrentAnalysis": 5,
    "DefaultCategoryConfidenceThreshold": 0.7,
    "SimilarityScoreThreshold": 0.8,
    "BulkAnalysisBatchSize": 100,
    "AutoApprovalThreshold": 0.95,
    "SupportedSites": [
      "amazon.com",
      "newegg.com",
      "bestbuy.com"
    ],
    "CategoryPrediction": {
      "UseMLClassifier": false,
      "FallbackToRuleEngine": true,
      "ModelPath": "/models/category-classifier.onnx"
    },
    "UrlAnalysis": {
      "RequestTimeout": "00:00:30",
      "UserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
      "MaxRetries": 3,
      "RetryDelay": "00:00:05"
    }
  },
  "Messaging": {
    "ScrapingExchange": "techticker.scraping",
    "AlertsExchange": "techticker.alerts",
    "PriceDataExchange": "techticker.pricedata",
    "ProductDiscoveryExchange": "techticker.productdiscovery",
    "ScrapeCommandQueue": "scrape.commands",
    "ScrapingResultQueue": "scraping.results",
    "RawPriceDataQueue": "pricedata.raw",
    "PricePointRecordedQueue": "pricedata.recorded",
    "AlertTriggeredQueue": "alerts.triggered",
    "ProductDiscoveryEventQueue": "productdiscovery.events",
    "ScrapeCommandRoutingKey": "scrape.command",
    "ScrapingResultRoutingKey": "scraping.result",
    "RawPriceDataRoutingKey": "pricedata.raw",
    "PricePointRecordedRoutingKey": "pricedata.recorded",
    "AlertTriggeredRoutingKey": "alert.triggered",
    "ProductDiscoveryEventRoutingKey": "productdiscovery.event"
  }
}
